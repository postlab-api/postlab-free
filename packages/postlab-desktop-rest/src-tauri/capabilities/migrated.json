{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:allow-write-file",
    "core:window:allow-start-dragging",
    "shell:allow-open",
    "dialog:allow-save",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://*/"
        },
        {
          "url": "https://*/"
        },
        {
          "url": "wss://*/"
        }
      ]
    },
    "os:allow-platform",
    "os:allow-version",
    "os:allow-os-type",
    "os:allow-family",
    "os:allow-arch",
    "os:allow-exe-extension",
    "os:allow-locale",
    "os:allow-hostname",
    "shell:default",
    "dialog:default",
    "fs:default",
    "os:default",
    "http:default",
    "store:default"
  ]
}